{
    "description": "Backup your persistent data and database in Google Cloud Storage",
    "type": "neos-package",
    "name": "neosrulez/backup-googlecloudstorage",
    "require": {
        "neos/flow": "^7.0 || ^8.0 || ^8.1",
        "wapmorgan/unified-archive": "^1.1",
        "ifsnop/mysqldump-php": "2.9",
        "google/cloud-storage": "*"
    },
    "autoload": {
        "psr-4": {
            "NeosRulez\\Backup\\GoogleCloudStorage\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "NeosRulez.Backup.GoogleCloudStorage"
        }
    }
}
